<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

	<preConditions>
			<dbms type="mysql" />
	</preConditions>
	<changeSet author="$Author$" id="$Rev$" dbms="mysql">
		<delete tableName="KRSB_SVC_DEF_T"></delete>

		<dropColumn tableName="krim_attr_defn_t" columnName="appl_url" />

		<addColumn tableName="KRSB_SVC_DEF_T">
			<column name="FLT_SVC_DEF_ID" type="BIGINT">
				<constraints nullable="false" />
			</column>
			<column name="SVC_DEF_CHKSM" type="VARCHAR(30)">
				<constraints nullable="false" />
			</column>
		</addColumn>

		<createIndex tableName="KRSB_SVC_DEF_T"
			indexName="KRSB_SVC_DEF_TI2">
			<column name="FLT_SVC_DEF_ID" />
		</createIndex>

		<createTable tableName="KRSB_FLT_SVC_DEF_T">
			<column name="FLT_SVC_DEF_ID" type="BIGINT"
				defaultValue="0">
				<constraints primaryKey="true"
					primaryKeyName="KRSB_FLT_SVC_DEF_TP1" nullable="false" />
			</column>
			<column name="FLT_SVC_DEF" type="LONGTEXT">
				<constraints nullable="false" />
			</column>
		</createTable>

		<createTable tableName="KRSB_FLT_SVC_DEF_S">
			<column autoIncrement="true" name="id" type="BIGINT">
				<constraints primaryKey="true" nullable="false" />
			</column>
		</createTable>
	</changeSet>
















</databaseChangeLog>
